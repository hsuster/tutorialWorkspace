<Window x:Class="tutorialWorkspace.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ahTest1"
        mc:Ignorable="d"
        Title="MainWindow" Height="215" Width="350">
    <Grid Margin="5,5,5,5">
        <Button x:Name="button_toCalc" Content="Calculator" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="button_toCalc_Click"/>
        <Button x:Name="button_close" Content="X" HorizontalAlignment="Left" Margin="297,10,0,0" VerticalAlignment="Top" Width="25" Foreground="Red" Click="button_close_Click"/>
    </Grid>
</Window>
